			//			Отказ = Ложь;
			//			ФормаЗачетаОплат.СохранитьРезультатЗачетаОплаты(МассивОбработанныхДокументов, Отказ);
			//		КонецЕсли;
			//	КонецЕсли; 
			//КонецЦикла; 
